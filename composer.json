{
    "name": "giudicelli/symfony-distributed-architecture-admin",
    "type": "symfony-bundle",
    "description": "Symfony distributed architecture admin",
    "keywords": [],
    "homepage": "https://github.com/giudicelli/symfony-distributed-architecture-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Giudicelli"
        }
    ],
    "repositories": [
        { "type": "vcs", "url": "git@github.com:giudicelli/symfony-distributed-architecture.git" }
    ],    
    "require": {
        "php": ">=7.2.5",
        "psr/log": "~1.0",
        "giudicelli/symfony-distributed-architecture": "dev-process-state-controller",
        "symfony/http-kernel": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "^5",
        "symfony/serializer": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "giudicelli\\DistributedArchitectureAdminBundle\\": "./" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "master-dev"
        }
    }
}

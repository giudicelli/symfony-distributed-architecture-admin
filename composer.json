{
    "name": "giudicelli/symfony-distributed-architecture-admin",
    "type": "symfony-bundle",
    "description": "Symfony distributed architecture admin",
    "keywords": [],
    "homepage": "https://github.com/giudicelli/symfony-distributed-architecture-admin",
    "license": "MIT",
    "authors": [
        {
            "name": "Frédéric Giudicelli"
        }
    ],
    "require": {
        "php": ">=7.2.5",
        "psr/log": "~1.0",
        "giudicelli/symfony-distributed-architecture": ">=0.5.0",
        "symfony/http-kernel": "^5",
        "symfony/dependency-injection": "^5",
        "symfony/framework-bundle": "^5",
        "symfony/orm-pack": "^1.0",
        "symfony/twig-bundle": "^5",
        "symfony/serializer": "^5",
        "symfony/validator": "^5"
    },
    "require-dev": {
        "phpunit/phpunit": "^9",
        "symfony/dotenv": "5",
        "symfony/flex": "^1.3.1",
        "doctrine/doctrine-fixtures-bundle": "^3.3@dev"
    },
    "conflict": {
    },
    "suggest": {
    },
    "autoload": {
        "psr-4": { "giudicelli\\DistributedArchitectureAdminBundle\\": "./" },
        "exclude-from-classmap": [
            "/Tests/"
        ]
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-master": "0.x-dev"
        }
    }
}
